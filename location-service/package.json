    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "knex": "^3.0.1",
    "pg": "^8.11.3",
    "@turf/turf": "^6.5.0",
    "geolib": "^3.3.4",
    "bull": "^4.12.0",
    "ioredis": "^5.3.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "axios": "^1.6.2",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "@googlemaps/google-maps-services-js": "^3.3.42",
    "@mapbox/polyline": "^1.1.1",
    "node-cron": "^3.0.3",

{
  "name": "location-service",
  "version": "1.0.0",
  "description": "Real-time location tracking and geospatial services for P2P delivery platform",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "docker:build": "docker build -t location-service .",
    "docker:run": "docker run -p 3008:3008 location-service"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",

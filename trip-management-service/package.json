{
  "name": "trip-management-service",
  "version": "1.0.0",
  "description": "P2P Delivery Platform - Trip Management Microservice",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
  },
  "keywords": [
    "p2p",
    "delivery",
    "trip",
    "management",
    "travel",
    "microservice",
    "api"
  ],
  "author": "P2P Delivery Platform",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "sequelize": "^6.35.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "geolib": "^3.3.4",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "uuid": "^9.0.1",
    "@googlemaps/google-maps-services-js": "^3.3.42",
    "@mapbox/mapbox-sdk": "^0.15.3",
    "@turf/turf": "^6.5.0",
    "node-cache": "^5.1.2",
    "lodash": "^4.17.21",
    "validator": "^13.11.0",
    "sanitize-html": "^2.11.0",
    "express-mongo-sanitize": "^2.2.0",
    "hpp": "^0.2.3",
    "cookie-parser": "^1.4.6",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "csv-parser": "^3.0.0",
    "json2csv": "^5.0.7",
    "pdf-lib": "^1.17.1",
    "qrcode": "^1.5.3",
    "jimp": "^0.22.10",
    "node-schedule": "^2.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "sequelize-cli": "^6.6.2",
    "@types/jest": "^29.5.8",
    "faker": "^5.5.3",
    "sinon": "^17.0.1",
    "nock": "^13.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/tests/**",
      "!src/config/**",
      "!src/app.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console": "warn",
      "prefer-const": "error",
      "no-unused-vars": "error"
    }
  }
}